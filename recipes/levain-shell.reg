[HKEY_CURRENT_USER\Software\Classes\Directory\Background\shell\@@shellID@@]
@="@@shellName@@"

[HKEY_CURRENT_USER\Software\Classes\Directory\Background\shell\@@shellID@@\command]
@="cmd /c pushd \"%v\" && @@shellCmd@@"

[HKEY_CURRENT_USER\Software\Classes\Directory\shell\@@shellID@@]
@="@@shellName@@"

[HKEY_CURRENT_USER\Software\Classes\Directory\shell\@@shellID@@\command]
@="cmd /c pushd \"%1\" && @@shellCmd@@"

